<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxlogin.WebController">
    <children>
      <ImageView fx:id="webimg" fitHeight="400.0" fitWidth="609.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../target/istockphoto-936991552-170667a.jpg" />
         </image>
      </ImageView>
          <Label fx:id="label" layoutX="310.0" layoutY="43.0" text="Welcome" textAlignment="CENTER" textFill="#ff8000">
              <font>
                  <Font name="System Bold" size="24.0" />
              </font>
          </Label>
          <Button fx:id="backbut" layoutX="408.0" layoutY="313.0" mnemonicParsing="false" onAction="#back" text="Back">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
          <Button fx:id="butlogout" layoutX="476.0" layoutY="315.0" mnemonicParsing="false" onAction="#logout" text="Log Out">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <TextField fx:id="url" layoutX="151.0" layoutY="227.0" prefHeight="26.0" prefWidth="425.0" />
      <Button fx:id="butnext" layoutX="338.0" layoutY="313.0" mnemonicParsing="false" onAction="#next" text="Next">
         <font>
            <Font name="System Bold" size="14.0" />
