<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxlogin.Scene2Controller">
   <children>
      <ImageView fx:id="water2" fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../target/water2.jpg" />
         </image>
      </ImageView>
      <Label fx:id="label" layoutX="143.0" layoutY="120.0" text="Welcome" textAlignment="CENTER" textFill="#ff8000">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <RadioButton fx:id="web" layoutX="143.0" layoutY="249.0" mnemonicParsing="false" onAction="#webswitch" prefHeight="26.0" prefWidth="106.0" text="Web URL" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="man" layoutX="314.0" layoutY="249.0" mnemonicParsing="false" onAction="#manswitch" prefHeight="26.0" prefWidth="106.0" text="Manual" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="app" layoutX="142.0" layoutY="213.0" mnemonicParsing="false" onAction="#cameraswitch" prefHeight="26.0" prefWidth="106.0" text="Camera" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </RadioButton>
      <Button fx:id="butlogout" layoutX="249.0" layoutY="295.0" mnemonicParsing="false" onAction="#logout" text="Log Out">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Label layoutX="142.0" layoutY="172.0" text="Choose how you want to get the item" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <RadioButton fx:id="text" layoutX="314.0" layoutY="213.0" mnemonicParsing="false" onAction="#textswitch" prefHeight="26.0" prefWidth="126.0" text="Enter Information" textFill="WHITE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></RadioButton>
   </children>
</AnchorPane>
