<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxlogin.ImageController">
    <children>
        <ImageView fx:id="s3img" fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../../../../target/wall5.jpg" />
            </image></ImageView>
        <Label fx:id="label" layoutX="14.0" layoutY="13.0" text="Welcome" textAlignment="CENTER" textFill="#ff8000">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
        <Button fx:id="backbut" layoutX="423.0" layoutY="16.0" mnemonicParsing="false" onAction="#back" text="Back">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
        <Button fx:id="butnext" layoutX="361.0" layoutY="16.0" mnemonicParsing="false" onAction="#next" text="Next">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
        <Button fx:id="butlogout" layoutX="483.0" layoutY="16.0" mnemonicParsing="false" onAction="#logout" text="Log Out">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
      <ImageView fx:id="imagemenu" fitHeight="336.0" fitWidth="359.0" layoutX="20.0" layoutY="48.0" pickOnBounds="true" preserveRatio="true" />
    </children>
</AnchorPane>
